{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "./public",
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": [
    "./config/externals.d.ts",
    "./src/**/*.tsx",
    "./src/**/*.ts"
  ]
}
